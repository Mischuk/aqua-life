.load-logo {
  position: absolute;
  left: 50%;
  top: 50%;
  width: 220px;
  height: 118px;
  margin-left -(@width/2)
  margin-top -(@height/2)
  background: url("../images/logo.png") 0 0 no-repeat;
  transition: all 0.5s ease-out;
}
.load-logo span {
  position: absolute;
  width: 8px;
  height: 8px;
  background-color: #16a6c6;
  border-radius: 50%;
  left: 50%;
  bottom: -15px;
}
.load-logo .first-i {
  margin-left: -18px;
  -webkit-animation-name: preload-anim;
          animation-name: preload-anim;
  -webkit-animation-duration: 1s;
          animation-duration: 1s;
  -webkit-animation-delay: 0;
          animation-delay: 0;
  -webkit-animation-timing-function: linear;
          animation-timing-function: linear;
  -webkit-animation-iteration-count: infinite;
          animation-iteration-count: infinite;
  -webkit-animation-direction: normal;
          animation-direction: normal;
}
.load-logo .second-i {
  margin-left: -5px;
  -webkit-animation-name: preload-anim3;
          animation-name: preload-anim3;
  -webkit-animation-delay: 0.5s;
          animation-delay: 0.5s;
  -webkit-animation-duration: 1s;
          animation-duration: 1s;
  -webkit-animation-timing-function: linear;
          animation-timing-function: linear;
  -webkit-animation-iteration-count: infinite;
          animation-iteration-count: infinite;
  -webkit-animation-direction: normal;
          animation-direction: normal;
}
.load-logo .third-i {
  margin-left: 8px;
  -webkit-animation-name: preload-anim3;
          animation-name: preload-anim3;
  -webkit-animation-duration: 1s;
          animation-duration: 1s;
  -webkit-animation-delay: 2s;
          animation-delay: 2s;
  -webkit-animation-timing-function: linear;
          animation-timing-function: linear;
  -webkit-animation-iteration-count: infinite;
          animation-iteration-count: infinite;
  -webkit-animation-direction: normal;
          animation-direction: normal;
}
/*!=================
  Base layout
====================*/
html,*, *:after, *:before
  box-sizing border-box
html,
body
  // height 100%
  min-width 940px
body
  position relative
.wrapper
  width $min-width
  position relative
  margin 0 auto
  height 100%

/*!=================
  Basic elements
====================*/
html
  regular()
  font-size 30px
  line-height 1
img
  display inline-block
  vertical-align middle
  max-width 100%
  max-height 100%
  width auto
  height auto
.inp-textarea, textarea
  max-height 400px
  max-width 100%
strong, b
  font-weight normal
  font-style normal
  bold()
