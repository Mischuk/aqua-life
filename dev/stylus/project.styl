@-webkit-keyframes preload-anim {
  0% {
    opacity: 1;
  }
  50% {
    opacity: 0.4;
  }
  100% {
    opacity: 1;
  }
}
@keyframes preload-anim {
  0% {
    opacity: 1;
  }
  50% {
    opacity: 0.4;
  }
  100% {
    opacity: 1;
  }
}
@-webkit-keyframes preload-anim3 {
  0% {
    opacity: 0.4;
  }
  50% {
    opacity: 1;
  }
  100% {
    opacity: 0.4;
  }
}
@keyframes preload-anim3 {
  0% {
    opacity: 0.4;
  }
  50% {
    opacity: 1;
  }
  100% {
    opacity: 0.4;
  }
}
@-webkit-keyframes move-down-anim {
  0% {
    bottom: 100px;
  }
  50% {
    bottom: 115px;
  }
  100% {
    bottom: 100px;
  }
}
@keyframes move-down-anim {
  0% {
    bottom: 100px;
  }
  50% {
    bottom: 115px;
  }
  100% {
    bottom: 100px;
  }
}
